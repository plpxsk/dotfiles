# This is Git's per-user configuration file.
[user]
	name = Paul Paczuski
	useConfigOnly = true

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[alias]
	s = status
	b = branch
	r = reflog
	su = status -uno

[credential]
	helper = cache --timeout 999999

# [core]
# 	editor = vim

[pull]

	rebase = false
[init]
	defaultBranch = main
